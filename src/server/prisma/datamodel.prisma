type User {
  id: ID! @id
  name: String!
  birthMonth: String!
  email: String! @unique
  password: String!
  blurbs: [Blurb!]!
  pics: [Pic!]!
  feedback: [Feedback!]!
}

type Blurb {
  id: ID! @id
  owner: User!
  text: String!
  ratings: [Rating!]!
}

type Pic {
  id: ID! @id
  owner: User!
  url: String!
  ratings: [Rating!]!
}

type Rating {
  id: ID! @id
  pic: Pic
  blurb: Blurb
  score: Int!
}

type Feedback {
  id: ID! @id
  contentOwner: User! @relation(name: "receiver")
  feedbackGiver: User! @relation(name: "giver")
  text: String!
  createdAt: DateTime! @createdAt
}