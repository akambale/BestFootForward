{
  "name": "BestFootForward",
  "version": "1.0.0",
  "description": "An app to help you improve your dating profile",
  "main": "server.js",
  "scripts": {
    "test": "npm start",
    "compile-all": "concurrently \"npm:build\" \"npm:compile-watch\"",
    "start-prod": "concurrently \"npm:build-prod\" \"node src/server.js\"",
    "build": "webpack --config webpack.dev.js -d --watch",
    "build-prod": "webpack --config webpack.prod.js",
    "start": "nodemon src/server.js",
    "compile-scss": "node-sass src/client/scss/main.scss dist/index.css",
    "compile-watch": "node-sass src/client/scss/main.scss dist/index.css --watch",
    "schema": "mysql -u root -p < src/db/schema.sql",
    "seed": "mysql -u root -p < src/db/seed.sql",
    "votes": "mysql -u root -p < src/db/seed-votes.sql"
  },
  "author": "Amogh Kambale",
  "license": "MIT",
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "@reach/router": "^1.2.1",
    "apollo-server": "^2.9.15",
    "axios": "^0.18.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "concurrently": "^4.1.0",
    "express": "^4.17.1",
    "graphql": "^14.5.8",
    "graphql-yoga": "^1.18.3",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "mysql": "^2.17.1",
    "prisma-binding": "^2.3.16",
    "prisma-client-lib": "^1.34.10",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-image-crop": "^8.4.2"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "eslint": "^5.13.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.7.0",
    "eslint-utils": ">=1.4.1",
    "node-sass": "^4.11.0",
    "nodemon": "^1.19.4",
    "prettier": "^1.16.4",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.3",
    "webpack-merge": "^4.1.5"
  }
}
